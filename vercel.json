{
  "version": 2,
  "rewrites": [
    { "source": "/components/(.*)", "destination": "/components/$1" }
  ],
  "headers": [
    {
      "source": "/components/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
} 